    <field name="model">motorcycle.registry</field>
    <field name="inherit_id" ref="motorcycle_registry.motorcycle_registry_view_form"/>
    <field name="arch" type="xml">
        <xpath expr="//group" position="before">
            <div class="oe_button_box" name="button_box">
                <button type="object" name="action_repair_order_tree" class="oe_start_button">
                    <!-- <field name="repair_ids" string="Repair Orders" /> -->
                </button>
            </div>
        </xpath>
    </field>
</record>
</odoo>